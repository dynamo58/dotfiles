{
  "editor.formatOnSave": true,
  "editor.selectionClipboard": false, // disable pasting with middle mouse button
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.detectIndentation": true,
  "editor.fontFamily": "'JetBrains Mono', 'DinaRemasterII', '', '', 'Hasklug Nerd Font', 'Iosevka', 'Iosevka'",
  "editor.fontWeight": "400",
  "editor.wordWrap": "on",
  "editor.minimap.enabled": false,
  "explorer.confirmDelete": false,
  "editor.suggestSelection": "first",
  "security.workspace.trust.untrustedFiles": "open",
  "extensions.ignoreRecommendations": true,
  "workbench.startupEditor": "none",
  "security.workspace.trust.startupPrompt": "never",
  "python.languageServer": "Jedi",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook",
    "*.pdf": "default",
    "*.docx": "default"
  },
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "editor.cursorBlinking": "solid",
  "editor.cursorStyle": "block-outline",
  "explorer.confirmDragAndDrop": false,
  "git.confirmSync": false,
  "security.workspace.trust.banner": "never",
  "terminal.integrated.fontSize": 12,
  "git.enableSmartCommit": true,
  "rust-analyzer.callInfo.full": false,
  "editor.guides.bracketPairsHorizontal": false,
  "editor.guides.highlightActiveBracketPair": false,
  "editor.fontLigatures": true,
  "security.workspace.trust.enabled": false,
  "editor.multiCursorModifier": "ctrlCmd",
  // "editor.rulers": [
  //     80,
  //     120
  // ],
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "deno.codeLens.test": false,
  "deno.enable": true,
  "rust-analyzer.cargo.allFeatures": true,
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": "off"
  },
  "editor.formatOnPaste": true,
  "editor.bracketPairColorization.enabled": false,
  "git.autofetch": true,
  "editor.renderWhitespace": "none",
  "editor.accessibilitySupport": "off",
  "editor.autoClosingBrackets": "always",
  "editor.autoClosingDelete": "always",
  "window.autoDetectHighContrast": false,
  "terminal.explorerKind": "external",
  "terminal.external.linuxExec": "wezterm",
  "search.showLineNumbers": true,
  "debug.showInlineBreakpointCandidates": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "gitlens.codeLens.enabled": false,
  "gitlens.statusBar.enabled": false,
  "liveServer.settings.donotShowInfoMsg": true,
  "workbench.iconTheme": "vscode-icons",
  "editor.renderLineHighlight": "none",
  "window.menuBarVisibility": "toggle",
  // "workbench.statusBar.visible": false,
  "workbench.colorCustomizations": {
    // "editor.background": "#080808"
  },
  "workbench.editor.empty.hint": "hidden",
  "editor.guides.indentation": false,
  "gitlens.currentLine.enabled": false,
  "accessibility.signals.lineHasBreakpoint": {
    "sound": "off"
  },
  "[python]": {
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.formatOnType": true,
    "editor.wordBasedSuggestions": "off"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "rust-analyzer.inlayHints.typeHints.enable": false,
  "rust-analyzer.inlayHints.parameterHints.enable": false,
  "rust-analyzer.inlayHints.chainingHints.enable": false,
  "rust-analyzer.inlayHints.closingBraceHints.enable": false,
  "rust-analyzer.inlayHints.renderColons": false,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "background.filepath": "/home/smolik/Media/Pictures/1690736142396028.png",
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorTheme": "Mokka: Green"
}
